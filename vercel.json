{
  "routes": [
      {
        "src": "/api/(.*)",
        "headers": [
          {
            "source": "/(.*)",
            "headers": [
              {
                "key": "access-control-allow-origin",
                "value": "*"
              }
            ]
          }
        ],
        "dest": "api/app.py"
      }
    ]
  }